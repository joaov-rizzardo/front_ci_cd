{
  "name": "06-github-actions-front-end",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "author": "Mario Souto <13791385+omariosouto@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf build && next build && next export && mv ./out ./build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint",
    "deploy": "yarn build && npx vercel ./build --confirm --no-clipboard",
    "deploy:prod": "yarn deploy --prod --token=$VERCEL_TOKEN",
    "deploy:staging": "yarn deploy --token=$VERCEL_TOKEN",
    "deploy:preview": "node ./scripts/pullRequestPreview.js",
    "test:e2e:open": "cypress open",
    "test:e2e": "cypress run --config-file cypress.config.js"
  },
  "devDependencies": {
    "cypress": "^13.7.2",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  }
}
